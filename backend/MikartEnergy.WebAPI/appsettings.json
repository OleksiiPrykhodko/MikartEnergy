{
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.json",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true,
          "formatter": "Serilog.Formatting.Json.JsonFormatter"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ]
  },

  "AllowedHosts": "*",
  "FrontendAppUrl": "http://localhost:4200",
  "FrontendAppTiaStConfigResultPagePath": "/shop/configurator",
  "EtimFilesFolderName": "Files",
  "EtimProductsXmlFile": "ETIM8_UA.xml",
  "EtimFeaturesAndValuesXmlFile": "ETIM_F_V.xml"

}
